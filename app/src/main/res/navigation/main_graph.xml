<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/mainFragment"
    tools:ignore="UnusedNavigation">


    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.aio_project.fragment.MainFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_mainFragment_to_aioDetailsFragment"
            app:destination="@id/aioDetailsFragment" />
        <action
            android:id="@+id/action_mainFragment_to_exploreFragment"
            app:destination="@id/exploreFragment" />
    </fragment>
    <fragment
        android:id="@+id/aioDetailsFragment"
        android:name="com.example.aio_project.fragment.AioDetailsFragment"
        android:label="AioDetailsFragment" />
    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.example.aio_project.fragment.ExploreFragment"
        android:label="ExploreFragment" >
        <action
            android:id="@+id/action_exploreFragment_to_aioDetailsFragment"
            app:destination="@id/aioDetailsFragment" />
    </fragment>
</navigation>